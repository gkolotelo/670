




void sendACK()
{

}



int ledCommand()
{
	int uiReceiveBuff1, uiReceiveBuff2;
	while (UART0_BRD_S1_RDRF(UART0) == 0);
	uiReceiveBuff1 = SCANF();
	while (UART0_BRD_S1_RDRF(UART0) == 0);
	uiReceiveBuff2 = SCANF();

}

int readCommand()
{
	int uiReceiveBuff;
	while (UART0_BRD_S1_RDRF(UART0) == 0);
	uiReceiveBuff = SCANF();
	switch(uiReceiveBuff)
	{
		case 76:
		case 108:
			return ledCommand();
	}
}
