#include "hal/uart/debugUart.h"
//#include "hal/uart/interpreter.h"

int boardInit()
{
    mcg_clockInit();
}

int peripheralInit()
{
	//interpreter_init();
}
int uiReceiveBuff;
int main(void)
{
    boardInit();
    peripheralInit();
    debugUart_init();


    for (;;) {
    	//interpreter_readCommand();

    	while (UART0_BRD_S1_RDRF(UART0) == 0);
    	uiReceiveBuff = GETCHAR();
    	PUTCHAR(uiReceiveBuff);


    }

    return 0;
}
